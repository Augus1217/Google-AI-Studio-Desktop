{
  "name": "google-ai-studio-desktop",
  "version": "1.5.0",
  "description": "Unofficial Google AI Studio Desktop Client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux",
    "build:mac": "electron-builder --mac"
  },
  "build": {
    "appId": "com.google.ai.studio.desktop",
    "productName": "Google AI Studio",
    "linux": {
      "icon": "icon.png",
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "zip"
      ],
      "category": "Development"
    },
    "win": {
      "icon": "icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "icon": "icon.png",
      "category": "public.app-category.developer-tools",
      "target": [
        "zip"
      ]
    }
  },
  "keywords": [],
  "author": "Augus <imlindora@gmail.com>",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12"
  },
  "optionalDependencies": {
    "dmg-license": "^1.0.11"
  }
}
